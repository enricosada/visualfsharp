  </ItemGroup>

  <Import Project="$(FSharpSourcesRoot)\FSharpSource.netcore.props" />

  <ItemGroup>
    <PackageReference Include="FSharp.NET.Sdk" Version="1.0.0-char-*">
      <PrivateAssets>All</PrivateAssets>
    </PackageReference>
  </ItemGroup>

</Project>
